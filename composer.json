    "homepage": "https://github.com/Hiagojzz/SSBoletos",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Hiago Junior",
            "email": "hiago_j@hotmail.com",
            "homepage": "http://hiagodev.com.br"
        }
    ],
    "support": {
        "issues": "https://github.com/Hiagojzz/SSBoletos/issues",
        "source": "https://github.com/hiagojzz/SSBoletos"
    },
    "require": {
        "php": ">=5.6",
        "ext-dom": "*",
        "ext-xmlwriter": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "fzaninotto/faker": "1.6.*",
        "phpunit/phpunit": "5.6.*",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "TIExpert\\WSBoletoSantander\\": "src/"
        }
    }
}
